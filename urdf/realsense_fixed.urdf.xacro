<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="realsense">
    <xacro:include filename="$(find doris_sensors_description)/urdf/realsense_d435_camera.urdf.xacro" />
    <xacro:property name="M_PI" value="3.1415926535897931"/>
    <xacro:realsense_d435_camera name="camera" parent="world">
        <origin rpy="0 ${M_PI/4} ${M_PI}" xyz="1.0 0.0 0.5"/>
    </xacro:realsense_d435_camera>
    <link name="world">
    </link>
</robot>
